#*--*##else
            ${member.name}.Initialize<${member.nativeTypeFQName}, $member.nativeTypeFQNameUnmanaged>(dimension: $member.arrayTotalSize, allocatePointers: allocatePointers, allocateMemory: allocateMemory);
