                    "${node.name}",
#*----*##generateTypecode($node.memberFieldMapList[0] true);
#*--*##else
#*----*##if($union)
            // ${node.name} union
            var ${node.name}StructMembers = new UnionMember[]
            {
#*----*##else
            // ${node.name} struct
            var ${node.name}StructMembers = new StructMember[]
            {
#*----*##end
#*----*##foreach($member in $node.typeCodeMemberFieldMapList)
